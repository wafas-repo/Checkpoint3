/* A program that takes 3 input values from user
   and outputs the smallest number.  */

int getSmallest(int a, int b, int c){
	int smallest;
	smallest = a;

	if (b < smallest)
		smallest = b;

	if (c < smallest)
		smallest = c;

	return smallest;
}

void main (void)
{
  int x; int y; int z;
  x = input();
  y = input();
  z = input();

  output(getSmallest(x, y,z));
}