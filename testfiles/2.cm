
/* A program that displays the Fibonacci sequence 
    of first n numbers (entered by the user)  */

void main(void)
{
    int i; int n; int t1; int t2; int nextTerm;
    i = 1;
    t1 = 0;
    t2 = 1;
    n = input();

    while(i<=n)
    {
        output(t1);
        nextTerm = t1 + t2;
        t1 = t2;
        t2 = nextTerm;
        i = i + 1;
    }
}